{
  "name": "psr-inventory-backend",
  "version": "1.0.0",
  "description": "Backend for PSR Inventory Management",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "node ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./knexfile.cjs",
    "rollback": "node ./node_modules/knex/bin/cli.js migrate:rollback --knexfile ./knexfile.cjs",
    "seed": "node ./node_modules/knex/bin/cli.js seed:run --knexfile ./knexfile.cjs"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.9",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
